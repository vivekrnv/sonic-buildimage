From d1a215a27f0bcc2733036857ddcd50cd43a75521 Mon Sep 17 00:00:00 2001
From: Oleksandr Ivantsiv <oivantsiv@nvidia.com>
Date: Thu, 4 May 2023 18:22:00 +0300
Subject: [PATCH] Disable FlexIO support.

---
 debian/rules | 15 ---------------
 1 file changed, 15 deletions(-)

diff --git a/debian/rules b/debian/rules
index 16f1796..bec4412 100755
--- a/debian/rules
+++ b/debian/rules
@@ -84,21 +84,6 @@ ifeq ($(OS_NAME), Ubuntu)
 	endif
 endif
 
-ifeq ($(OS_NAME), Debian)
-	ifeq ($(OS_VERSION), 10.8)
-		ifeq ($(ARCH_CPU), x86_64)
-			DPA_SUPPORT=TRUE
-			FLEXIO_APP_SUPPORT=TRUE
-		endif
-	endif
-	ifeq ($(OS_VERSION), 11)
-		ifeq ($(ARCH_CPU), aarch64)
-			DPA_SUPPORT=TRUE
-			FLEXIO_APP_SUPPORT=TRUE
-		endif
-	endif
-endif
-
 # DOCA GPUNetIO apps and samples are enabled on: HOST x86_64: Debian packages
 GPU_SUPPORT := FALSE
 ifeq ($(ARCH_CPU), x86_64)
