DPATH       := $($(DOCKER_AUDITD_WATCHDOG)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/docker-auditd-watchdog.mk rules/docker-auditd-watchdog.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
DEP_FILES   += $(shell git ls-files $(DPATH))

$(DOCKER_AUDITD_WATCHDOG)_CACHE_MODE  := GIT_CONTENT_SHA
$(DOCKER_AUDITD_WATCHDOG)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(DOCKER_AUDITD_WATCHDOG)_DEP_FILES   := $(DEP_FILES)

$(eval $(call add_dbg_docker,$(DOCKER_AUDITD_WATCHDOG),$(DOCKER_AUDITD_WATCHDOG_DBG)))
