DPATH       := $($(DOCKER_AUDITD)_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/docker-auditd.mk rules/docker-auditd.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
